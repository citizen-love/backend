{
  "name": "citizen-love-serverless",
  "version": "1.0.0",
  "apiVersion": "v0.1",
  "description": "Collection of google functions used at Citizen Love",
  "main": "functions.js",
  "scripts": {
    "package:local": "webpack --env.NODE_ENV=local",
    "package:staging": "webpack --env.NODE_ENV=development",
    "package:prod": "webpack --env.NODE_ENV=production",

    "dev": "nodemon --exec run-s package:local start-functions",
    "start-functions": "functions-framework --target=api --source=.webpack/functions.js",
    
    "tunnel": "ngrok http 8081"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citizen-love/backend"
  },
  "dependencies": {
    "actions-on-google": "^2.12.0",
    "axios": "0.19.0",
    "cors": "^2.8.5",
    "dialogflow-fulfillment": "0.6.1",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "firebase-admin": "8.7.0",
    "firebase-functions": "3.3.0",
    "geo-distance": "^0.2.0",
    "geofirestore": "^3.4.1",
    "twilio": "3.40.0",
    "uuid": "^7.0.2",
    "uuid-random": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@google-cloud/functions-framework": "^1.5.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "cross-env": "6.0.3",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-promise": "4.2.1",
    "ngrok": "3.2.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "path": "0.12.7",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "author": "Citizen-love",
  "license": "ISC",
  "engines": {
    "node": "8"
  },
  "bugs": {
    "url": "https://github.com/citizen-love/backend/issues"
  },
  "homepage": "https://github.com/citizen-love/backend#readme"
}
